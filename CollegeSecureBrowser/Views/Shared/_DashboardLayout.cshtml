<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] | Exam Proctor</title>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">


    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body id="Body" onload="SetNavbar()">

    <nav class="navbar navbar-expand-lg border-16 shadow-3 bg-white-color z-index-10 p-1 py-0">
        <a class="navbar-brand" href="#"><img src="~/img/Logo1.svg" alt="Logo" height="36" class="smooth-transition"></a>
        <div class="collapse navbar-collapse mx-4" id="navbarNavAltMarkup">
            <div class="navbar-nav">
                @*<a class="nav-item nav-link" href="/Dashboard/" id="Home">Home</a>*@
                <a class="nav-item nav-link" href="/Dashboard/Students" id="Students">Students</a>
                <a class="nav-item nav-link" href="/Dashboard/Exams" id="Exams">Exams</a>
                <a class="nav-item nav-link" href="/Dashboard/Settings" id="Settings">Settings</a>
            </div>
        </div>
@*        <div class="navbar-nav float-right">
            <a class="nav-item nav-link active cursor-pointer" onclick="Logout()">Logout</a>
        </div>*@
    </nav>



    <div class="dropdown Header-User d-none z-index-10">
        <button class="btn btn-secondary bg-white-color dropdown-btn fg-text-color dropdown-toggle border-16 border-0 shadow-4 shadow-1-hover smooth-transition" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <span id="UsernameDropdown" class="mr-2">

            </span>
        </button>

        <div class="dropdown-menu fade bg-secondary-background border-16 border-0 shadow-2" aria-labelledby="dropdownMenuButton">

            <a class="dropdown-item bg-secondary-background smooth-transition border-12 fg-text-color fg-hover-primary-color bg-hover-primary-color" href="/Dashboard/Settings">
                <svg width="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <defs>
                        <style>
                            .cls-1 {
                                fill: none;
                            }

                            .cls-3 {
                                fill: var(--text-color);
                                stroke: var(--text-color);
                                stroke-width: 0px;
                            }
                        </style>
                    </defs>
                    <path fill-rule="evenodd" clip-rule="evenodd" class="cls-3 smooth-transition" d="M11.1987 2.5872C11.697 2.31035 12.303 2.31035 12.8013 2.5872L20.0013 6.5872C20.5251 6.87821 20.85 7.43034 20.85 8.02956V15.9704C20.85 16.5697 20.5251 17.1218 20.0013 17.4128L12.8013 21.4128C12.303 21.6896 11.697 21.6897 11.1987 21.4128L3.99869 17.4128C3.47487 17.1218 3.15 16.5697 3.15 15.9704V8.02956C3.15 7.43034 3.47487 6.87821 3.99869 6.5872L11.1987 2.5872ZM12.0728 3.89844C12.0275 3.87327 11.9725 3.87327 11.9272 3.89844L4.72715 7.89844C4.67953 7.92489 4.65 7.97509 4.65 8.02956V15.9704C4.65 16.0249 4.67953 16.0751 4.72715 16.1016L11.9272 20.1016C11.9725 20.1267 12.0275 20.1267 12.0728 20.1016L19.2728 16.1016C19.3205 16.0751 19.35 16.0249 19.35 15.9704V8.02956C19.35 7.97509 19.3205 7.92489 19.2728 7.89844L12.0728 3.89844Z" fill="black" />
                    <path fill-rule="evenodd" clip-rule="evenodd" class="cls-3 smooth-transition" d="M7.25 12C7.25 9.37665 9.37665 7.25 12 7.25C14.6234 7.25 16.75 9.37665 16.75 12C16.75 14.6234 14.6234 16.75 12 16.75C9.37665 16.75 7.25 14.6234 7.25 12ZM12 8.75C10.2051 8.75 8.75 10.2051 8.75 12C8.75 13.7949 10.2051 15.25 12 15.25C13.7949 15.25 15.25 13.7949 15.25 12C15.25 10.2051 13.7949 8.75 12 8.75Z" fill="black" />
                </svg>
                Settings
            </a>

            <a onclick="Logout()" class="dropdown-item bg-secondary-background smooth-transition border-12 fg-text-color fg-hover-primary-color bg-hover-primary-color cursor-pointer">
                <svg xmlns="http://www.w3.org/2000/svg" height="20" viewBox="0 0 24 24">
                    <defs>
                        <style>
                            .cls-1 {
                                fill: none;
                            }

                            .cls-3 {
                                fill: var(--text-color);
                                stroke: var(--text-color);
                            }

                            .sw-05 {
                                stroke-width: 0.5px;
                            }
                        </style>
                    </defs>
                    <g>
                        <g>
                            <g>
                                <rect class="cls-1" width="24" height="24" />
                                <path class="cls-3 sw-05 smooth-transition" d="M20.75,5.54V18.46a2.32,2.32,0,0,1-.68,1.63,2.28,2.28,0,0,1-1.63.68H11.05a2.3,2.3,0,0,1-2.3-2.31V14.77h.92v3.69a1.42,1.42,0,0,0,.4,1,1.4,1.4,0,0,0,1,.41h7.39a1.38,1.38,0,0,0,1.38-1.39V5.54a1.38,1.38,0,0,0-1.38-1.39H11.05a1.4,1.4,0,0,0-1,.41,1.42,1.42,0,0,0-.4,1V9.23H8.75V5.54a2.3,2.3,0,0,1,2.3-2.31h7.39a2.28,2.28,0,0,1,1.63.68A2.32,2.32,0,0,1,20.75,5.54Z" />
                                <path class="cls-3 sw-05 smooth-transition" d="M3.26,11.92a.72.72,0,0,1,.19-.34l.15-.14L6.06,9a.49.49,0,0,1,.15-.1.39.39,0,0,1,.18,0,.49.49,0,0,1,.3.14.36.36,0,0,1,.1.15.43.43,0,0,1,0,.36.44.44,0,0,1-.1.15L4.85,11.52H12a.46.46,0,0,1,.32.13.46.46,0,0,1,0,.66.46.46,0,0,1-.32.13H4.79l1.9,1.93a.36.36,0,0,1,.1.15.43.43,0,0,1,0,.36.44.44,0,0,1-.1.15.4.4,0,0,1-.15.1.47.47,0,0,1-.17,0,.58.58,0,0,1-.18,0,.45.45,0,0,1-.14-.1L3.43,12.41a.64.64,0,0,1-.14-.22A.63.63,0,0,1,3.26,11.92Z" />
                            </g>
                        </g>
                    </g>
                </svg>
                Logout
            </a>

        </div>
    </div>

    <div class="container-fluid ">
        <main role="main" class="DashboardContainer">
            @RenderBody()
        </main>
    </div>

    <div id="topNotch"></div>

    <div id="title-bar-btns">
        <button id="min-btn" onclick="MinimizeWindow()" class="bg-yellow border-0 border-50 w-20px h-20px smooth-transition shadow-yellow mx-1 bg-yellow-hover shadow-yellow-hover">
        </button>

        <button id="max-btn" onclick="MaximizeeWindow()" class="bg-green border-0 border-50 w-20px h-20px smooth-transition shadow-green mx-1 bg-green-hover shadow-green-hover">
        </button>

        <button id="close-btn" onclick="CloseWindow()" class="bg-red border-0 border-50 w-20px h-20px smooth-transition shadow-red mx-1 bg-red-hover shadow-red-hover">
        </button>
    </div>

    <div class="alert container alert-dismissible fade mt-3 position-fixed border-10 smooth-transition user-select-none justify-content-center mx-auto text-white shadow w-75" role="alert" id="AlertMsg" style="transform:translateX(-50%); left:50%">
        <strong id="DisplayAlertHeading" class="letter-spacing-1 mr-1"></strong>
        <span id="DisplayAlertContent"></span>
        <button type="button" class="close smooth-transition text-white p-0 mr-2 border-primary-color" style="top: 50%; transform: translateY(-55%);" onclick="removeDisplayMsg(10)" aria-label="Close">
            <span aria-hidden="true" class="px-2 pt-0 pb-1 bg-opacity-05 border-radius-8">&times;</span>
        </button>
    </div>
    <script src="~/js/Extra.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    <script src="~/js/popper.min.js"></script>

    <script defer src="~/js/site.js" asp-append-version="true"></script>
    <script defer src="~/js/DropdownSelect.js" asp-append-version="true"></script>

    <script defer src="~/js/navbar.js"></script>
    <script defer src="~/js/Dashboard.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

